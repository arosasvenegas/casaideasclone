{
    "header-layout.mobile": {
        "title": "Header Mobile",
        "children": [
          "header-row#header__top",
          "header-row#header__content",
          "header-row#header__middle"
        ]
      },
      "header-row#header__top": {
        "children": [
          "rich-text#header__top--text"
        ],
        "props": {
          "blockClass": "header__top"
        }
      },
      "rich-text#header__top--text": {
        "props": {
          "blockClass": "header__top--text",
          "text": "¡Este fin de semana largo aprovecha el despacho gratis por compras $50.000!",
          "textAlignment": "CENTER",
          "textPosition": "CENTER"
        }
      },
      "header-row#header__content": {
        "children": [
          "logo#header__top--logo",
          "header-spacer",
          "search-bar#header__search",
          "locale-switcher",
          "icon#home__delivery--icon",
          "icon#home__stores--icon",
          "login",
          "minicart.v2"
        ],
        "props": {
          "blockClass": "header__content"
        }
      },
      "logo#header__top--logo": {
        "title": "Logo",
        "props": {
          "url": "assets/img/logo/header__logo.png",
          "width": 189,
          "href": "/"
        }
      },
      "icon#home__delivery--icon": {
        "props": {
          "id": "mpa-truck",
          "size": 16
        }
      },
      "icon#home__stores--icon": {
        "props": {
          "id": "mpa-store",
          "size": 16
        }
      },
      "login": {
        "props": {
          "emailAndPasswordTitle": "INGRESA A TU CUENTA¡Nos alegra tenerte de vuelta!",
          "accessCodePlaceholder": "Access Code LOG-IN",
          "emailPlaceholder": "e-mail",
          "passwordPlaceholder": "password",
          "showPasswordVerificationIntoTooltio": true,
          "showIconProfile": true
        }
      },
      "header-row#header__middle": {
        "title": "Header Medio",
        "children": [
            "drawer#header__mobile--menu"
            
        ],
        "props": {
          "blockClass": "header__middle"
        }
      },
      "autocomplete-result-list.v2": {
        "blocks": ["product-summary"]
      },
      "search-bar#header__search": {
        "blocks": ["autocomplete-result-list.v2"],
        "props": {
            "openAutocompleteOnFocus": true
        }
      }
     
     
      
      
      
}