{
  "flex-layout.row#desktop__results--search": {
    "title": "result search container",
    "children": [
      "list-context.product-list#desktop__results--search"
    ],
    "props": {
      "blockClass": "desktop__results--search"
    }
  },
  "list-context.product-list#desktop__results--search": {
    "title": "Result search list context",
    "blocks": [
      "product-summary.shelf#listLayout"
    ],
    "children": [
      "slider-layout#listLayout"
    ],
    "props": {
      "blockClass": "listLayout"
    }
  },
  "product-summary.shelf#listLayout": {
    "title": "Product summary shelf list Layout",
    "children": [
      "add-to-list-btn",
      "product-summary-image",
      "product-summary-name",
      "product-summary-space",
      "product-summary-price",
      "add-to-cart-button"
    ]
  },
  "slider-layout#listLayout": {
    "title": "Slider list layout",
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 1
      },
      "showNavigationArrows": "desktopOnly",
      "showPaginationDots": "always",
      "infinite": false,
      "fullWidth": true,
      "blockClass": "shelf"
    }
  }
}