  "product-summary.shelf#home__shelf--products-desktop": {
    "children": [
      "add-to-list-btn",
      "product-summary-image",
      "product-summary-name",
      "product-rating-inline",
      "product-list-price",
      "product-selling-price",
      "product-summary-sku-selector",
      "add-to-cart-button",
      "modal-trigger#global__quick-view"
    ],
    "props": {
      "blockClass": "product__card"
    }
  },
  "product-summary-image": {
    "props": {
      "blockClass": "product-summary-image"
    }
  },
  "product-summary-name": {
    "props": {
      "tag": "h3"
    }
  },
  "product-summary-sku-selector": {
    "props": {
      "hideImpossibleCombinations": false
    }
  },
  "slider-layout#home__slider--products-desktop": {
    "props": {
      "blockClass": "home__slider--products-desktop",
      "itemsPerPage": {
        "desktop": 3,
        "tablet": 4,
        "phone": 2
      },
      "showNavigationArrows": "always",
      "infinite": true
    }
  },
  "modal-trigger#global__quick-view": {
    "children": [
      "rich-text#global__quick-view--trigger",
      "modal-layout#global__quick-view--modal"
    ]
  },
  "rich-text#global__quick-view--trigger": {
    "props": {
      "text": "vista r√°pida",
      "blockClass": "quick-view__trigger"
    }
  },
  "modal-layout#global__quick-view--modal": {
    "children": [
      "modal-header#quick-view__header",
      "flex-layout.row#quick-view__modal"
    ]
  },
  "modal-header#quick-view__header": {
    "children": [],
    "props": {
      "showCloseButton": true,
      "iconCloseSize": 25
    }
  },
  "flex-layout.row#quick-view__modal": {
    "children": [
      "flex-layout.col#quick-view__modal--image",
      "flex-layout.col#quick-view__modal--info"
    ]
  },
  "flex-layout.col#quick-view__modal--image": {
    "children": [
      "product-summary-image"
    ]
  },
  "flex-layout.col#quick-view__modal--info": {
    "children": [
      "add-to-list-btn",
      "product-summary-name",
      "product-summary-price",
      "product-description",
      "share",
      "product-quantity",
      "add-to-cart-button"
    ]
  }
}